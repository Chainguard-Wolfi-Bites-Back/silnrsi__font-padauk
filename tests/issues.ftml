  <testgroup label="#38 Need a dotted form of U+1002">
    <test label="U+1000">
      <comment>ka ka-vs1</comment>
      <string>\u1000 \u1000\ufe00</string>
    </test>
    <test label="U+1002">
      <comment>ga ga-vs1</comment>
      <string>\u1002 \u1002\ufe00</string>
    </test>
  </testgroup>
  <testgroup label="#45 Missing glyph and not rendered as expected for Tai Laing">
    <test label="U+A9E4">
      <comment>bhaShan</comment>
      <string>\uA9E4</string>
    </test>
    <test label="U+A9E8">
      <comment>faTaiLaing</comment>
      <string>\uA9E8</string>
    </test>
  </testgroup>
  <testgroup label="https://github.com/harfbuzz/harfbuzz/discussions/4784 Burmese script marking character placement problem with Harfbuzz / Speedata">
    <test label="word">
      <string>သာရ္ဒ္ဓံ</string>
    </test>
    <test label="phrase">
